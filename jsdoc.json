{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./lib",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/braintree-jsdoc-template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "referenceTitle": "Segment API Wrapper by Perspective",
    "disableSort": false
  }
}
